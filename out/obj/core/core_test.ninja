defines = -DNDEBUG -DUNICODE -D_UNICODE -DWINVER=0x0A00 -D_WIN32_WINNT=0x0A00 -D_WIN32_WINDOWS=0x0A00 -DNTDDI_VERSION=0x0A000000 -DWIN32_LEAN_AND_MEAN -D_CRT_NONSTDC_NO_DEPRECATE -D_CRT_NONSTDC_NO_WARNINGS -D_CRT_SECURE_NO_DEPRECATE -D_CRT_SECURE_NO_WARNINGS
include_dirs = -I.. -Igen -Iglog/include -Igbenchmark/include -Igtest/include -Igen -Iglog/include -Igbenchmark/include -Igtest/include -I.. -I.. -I.. -I.. -I.. -I.. -I.. -I.. -I..
cflags = /EHsc /Gd /Gm- /Oy- /W3 /WX- /Zi /Zl /GS /Zc$:forScope /Zc$:wchar_t /analyze- /bigobj /fp$:precise /analyze- /bigobj /fp$:precise /utf-8 /Gy /O2 /Oi /GL /MT
cflags_cc = /std$:c++latest /EHsc
label_name = core_test
target_out_dir = obj/core
target_output_name = core_test

build obj/core/core_test.inputdeps.stamp: stamp obj/third_party/gbenchmark.stamp obj/third_party/glog.stamp obj/third_party/gtest.stamp
build obj/core/core_test/core_unittest.obj: cxx ../core/core_unittest.cpp || obj/core/core_test.inputdeps.stamp
build obj/core/core_test/main.obj: cxx ../testing/main.cpp || obj/core/core_test.inputdeps.stamp

build ./core_test.exe ./core_test.exe.pdb: link obj/core/core_test/core_unittest.obj obj/core/core_test/main.obj ./core.lib ./base.lib | glog/lib/glog.lib gbenchmark/lib/benchmark.lib gbenchmark/lib/benchmark_main.lib gtest/lib/gmock.lib gtest/lib/gmock_main.lib gtest/lib/gtest.lib gtest/lib/gtest_main.lib || obj/third_party/gbenchmark.stamp obj/third_party/gtest.stamp
  ldflags = /DEBUG /NODEFAULTLIB /MANIFEST$:EMBED /INCREMENTAL$:NO /OPT$:ICF /OPT$:REF /LTCG /MACHINE$:X64
  libs = glog/lib/glog.lib gbenchmark/lib/benchmark.lib gbenchmark/lib/benchmark_main.lib gtest/lib/gmock.lib gtest/lib/gmock_main.lib gtest/lib/gtest.lib gtest/lib/gtest_main.lib oldnames.lib libcmt.lib libcpmt.lib libvcruntime.lib libucrt.lib kernel32.lib user32.lib gdi32.lib gdiplus.lib winspool.lib comdlg32.lib advapi32.lib shell32.lib ole32.lib oleaut32.lib uuid.lib odbc32.lib odbccp32.lib comctl32.lib msimg32.lib winmm.lib shlwapi.lib Imm32.lib Dbghelp.lib
  output_extension = .exe
  output_dir = .
